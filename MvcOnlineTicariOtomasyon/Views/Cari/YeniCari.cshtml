@model MvcOnlineTicariOtomasyon.Models.NewFolder1.siniflar.Cariler
@{
    ViewBag.Title = "YeniCari";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<br />
<h2>Yeni Cari Sayfası</h2>

<br />
<h2>Yeni Departman Sayfası</h2>
<br />
<form class="form-group" method="post">

    @Html.LabelFor(x => x.CariAd)
    @Html.TextBoxFor(x => x.CariAd, new { @class = "form-control" })
    <br />

    @Html.LabelFor(x => x.CarSoyad)
    @Html.TextBoxFor(x => x.CarSoyad, new { @class = "form-control" })
    <br />

    @Html.LabelFor(x => x.CariSehri)
    @Html.TextBoxFor(x => x.CariSehri, new { @class = "form-control" })

    <br />

    @Html.LabelFor(x => x.CariMail)
    @Html.TextBoxFor(x => x.CariMail, new { @class = "form-control" })

    <br />







    <button class="btn btn-info">Kaydet</button>

</form>